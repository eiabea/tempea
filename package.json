{
  "name": "tempea",
  "version": "0.3.4",
  "description": "Node.js application for controlling room temperature via raspberry pi",
  "main": "app.js",
  "private": true,
  "scripts": {
    "precommit": "npm run lint",
    "start": "node app.js",
    "start:dev": "${NODE_PATH:-node_modules}/.bin/nodemon app.js",
    "lint": "${NODE_PATH:-node_modules}/.bin/eslint --fix .",
    "test": "${NODE_PATH:-node_modules}/.bin/nyc ${NODE_PATH:-node_modules}/.bin/_mocha --color --exit test/**/*.test.js",
    "coveralls": "cat ./coverage/lcov.info | ${NODE_PATH:-node_modules}/.bin/coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.eiabea.com/eiabea/tempea.git"
  },
  "author": "eiabea",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.eiabea.com/eiabea/tempea/issues"
  },
  "dependencies": {
    "@aclify/aclify": "^1.3.1",
    "body-parser": "^1.18.2",
    "bunyan": "^1.8.12",
    "chai": "^4.1.2",
    "cors": "^2.8.4",
    "cron": "^1.3.0",
    "ds18b20": "^0.1.0",
    "express": "^4.16.3",
    "express-rate-limit": "^2.11.0",
    "google-auth-library": "^1.4.0",
    "googleapis": "^29.0.0",
    "helmet": "^3.12.0",
    "influxdb-nodejs": "^2.7.6",
    "jsonwebtoken": "^8.2.1",
    "moment": "^2.22.1",
    "onoff": "^3.0.2",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "redis": "^2.8.0",
    "request": "^2.85.0"
  },
  "devDependencies": {
    "chai-http": "^4.0.0",
    "coveralls": "^3.0.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-chai": "^0.0.1",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-mocha": "^5.0.0",
    "husky": "^0.14.3",
    "mocha": "^5.1.1",
    "nock": "^9.2.5",
    "node-mocks-http": "^1.6.6",
    "nodemon": "^1.17.3",
    "null-logger": "^1.0.0",
    "nyc": "^11.7.1"
  }
}
