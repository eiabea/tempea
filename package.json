{
  "name": "tempea",
  "version": "0.3.3",
  "description": "Node.js application for controlling room temperature via raspberry pi",
  "main": "app.js",
  "private": true,
  "scripts": {
    "precommit": "npm run lint",
    "start": "${NODE_PATH:-node_modules}/.bin/node app.js",
    "start:dev": "${NODE_PATH:-node_modules}/.bin/nodemon app.js",
    "lint": "${NODE_PATH:-node_modules}/.bin/eslint --fix .",
    "test": "npm run test:unit",
    "test:unit": "nyc --reporter lcov --reporter text ${NODE_PATH:-node_modules}/.bin/_mocha test ./test/**/*.test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.eiabea.com/eiabea/tempea.git"
  },
  "author": "eiabea",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.eiabea.com/eiabea/tempea/issues"
  },
  "dependencies": {
    "acl": "^0.4.11",
    "body-parser": "^1.18.2",
    "bunyan": "^1.8.12",
    "bunyan-middleware": "^0.8.0",
    "cors": "^2.8.4",
    "cron": "^1.3.0",
    "ds18b20": "^0.1.0",
    "express": "^4.16.2",
    "express-rate-limit": "^2.9.0",
    "google-auth-library": "^0.11.0",
    "googleapis": "^22.2.0",
    "helmet": "^3.9.0",
    "influxdb-nodejs": "^2.7.4",
    "jsonwebtoken": "^8.1.1",
    "lodash": "^4.17.4",
    "moment": "^2.19.1",
    "onoff": "^1.1.8",
    "passport": "^0.4.0",
    "passport-jwt": "^3.0.1",
    "redis": "^2.8.0",
    "request": "^2.83.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "eslint": "^4.16.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-chai": "^0.0.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-mocha": "^4.11.0",
    "husky": "^0.14.3",
    "mocha": "^4.1.0",
    "node-mocks-http": "^1.6.6",
    "nodemon": "^1.12.1",
    "null-logger": "^1.0.0",
    "nyc": "^11.4.1",
    "unirest": "^0.5.1"
  }
}
